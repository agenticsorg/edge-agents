# DevOps Agent Environment Configuration
# Copy this file to .env and fill in your values

# AWS Configuration
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_SESSION_TOKEN=your-session-token-if-using-temporary-credentials
AWS_REGION=us-east-1
AWS_PROFILE=default

# GitHub Configuration
GITHUB_TOKEN=your-github-personal-access-token
GITHUB_ORG=your-github-organization

# Agent Configuration
DEVOPS_AGENT_LOG_LEVEL=INFO
DEVOPS_AGENT_CACHE_TTL=60
DEVOPS_AGENT_DEFAULT_TAGS={"Project":"DevOpsAgent","Environment":"Dev"}

# AWS Service-specific Configuration
DEVOPS_AGENT_EC2_DEFAULT_INSTANCE_TYPE=t2.micro
DEVOPS_AGENT_EC2_DEFAULT_AMI_ID=ami-0c55b159cbfafe1f0
DEVOPS_AGENT_S3_DEFAULT_REGION=us-east-1

# GitHub Service-specific Configuration
DEVOPS_AGENT_GITHUB_API_URL=https://api.github.com
DEVOPS_AGENT_GITHUB_USE_AGENT_ENDPOINT=false
DEVOPS_AGENT_GITHUB_AGENT_URL=

# Security Configuration
DEVOPS_AGENT_USE_KEYRING=true

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o

# OpenAI Agents SDK Configuration
DEVOPS_AGENT_TRACING_ENABLED=false
DEVOPS_AGENT_DEFAULT_AGENT=devops