{
  "tasks": {
    "build": "deno bundle src/index.ts > index.ts",
    "deploy": "deno task build && supabase functions deploy edge_deployment",
    "dev": "deno run --allow-net --allow-env --allow-read src/index.ts"
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.window", "deno.ns", "dom"],
    "strict": true,
    "target": "esnext"
  },
  "importMap": "./import_map.json"
}
