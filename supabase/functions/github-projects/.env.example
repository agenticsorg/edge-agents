# Required variables
GITHUB_TOKEN=your_github_token_here
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
GITHUB_ORG=your_github_org_here

# Server configuration
MCP_PORT=3000
MCP_HOST=0.0.0.0
API_BASE_PATH=/api

# Security settings
VALIDATE_TOKENS=true
API_TOKEN=your_api_token_here
CORS_ALLOWED_ORIGINS=*
ENABLE_TLS=false

# Logging settings
LOG_LEVEL=info
LOG_FORMAT=text

# Rate limiting
GITHUB_RATE_LIMIT=5000
API_RATE_LIMIT=60

# Cache settings
ENABLE_CACHE=true
CACHE_TTL=300

# Advanced settings
DEBUG_MODE=false
MAX_REQUEST_SIZE=5
WORKER_THREADS=0