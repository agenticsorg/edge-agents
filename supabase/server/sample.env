# PostgreSQL Configuration
POSTGRES_PASSWORD=postgres
POSTGRES_USER=postgres
POSTGRES_DB=postgres
POSTGRES_PORT=5432
POSTGRES_HOST=localhost

# Supabase Configuration
SUPABASE_URL=http://localhost:8000
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret
SUPABASE_ADMIN_EMAIL=admin@example.com
SUPABASE_ADMIN_PASSWORD=admin-password

# Edge Functions Configuration
EDGE_FUNCTIONS_PORT=9000
EDGE_RUNTIME=deno
DENO_PATH=/usr/bin/deno
EDGE_FUNCTIONS_PATH=/supabase/functions

# Storage Configuration
STORAGE_BACKEND=file
STORAGE_FILE_BACKEND_PATH=/var/lib/storage

# Auth Configuration
SITE_URL=http://localhost:3000
ADDITIONAL_REDIRECT_URLS=
JWT_EXPIRY=3600
DISABLE_SIGNUP=false

# SMTP Configuration for Auth
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
SMTP_SENDER_NAME=
SMTP_ADMIN_EMAIL=

# API Rate Limiting
API_RATE_LIMIT_REQUESTS=100
API_RATE_LIMIT_WINDOW=60

# Logging
LOG_LEVEL=info

# Security
ENABLE_SSL=false
SSL_CERT_PATH=
SSL_KEY_PATH=

# Realtime Configuration
REALTIME_ENABLED=true
